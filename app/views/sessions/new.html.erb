<h1>Log In To Stream Tracker</h1>

<%=render(partial: "application/errors", locals: {errors: @errors})%>
<%= form_for :user do |f|%>
  <%=f.label "Username:" %>
  <%=f.text_field :username%>
  <%=f.label "Password:"%>
  <%=f.password_field :password %>
  <%=f.submit "Log In"%>
<%end%>

<%= button_to "Log In With Google", "/auth/google_oauth2"%>